{
  "name": "@teknologi-umum/nedb-promises",
  "version": "5.0.3",
  "description": "A dead-simple promise wrapper for nedb.",
  "type": "module",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "files": [
    "README.md",
    "LICENSE",
    "index.js",
    "index.d.ts",
    "src/"
  ],
  "scripts": {
    "docs": "jsdoc2md ./src/*.js > docs.md --no-gfm",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js ",
    "test:watch": "jest --watchAll",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teknologi-umum/nedb-promises.git"
  },
  "keywords": [
    "nedb",
    "promises",
    "promise"
  ],
  "author": "Kristóf Baján",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teknologi-umum/nedb-promises/issues"
  },
  "homepage": "https://github.com/teknologi-umum/nedb-promises#readme",
  "dependencies": {
    "@seald-io/nedb": "^2.2.1"
  },
  "devDependencies": {
    "@teknologi-umum/eslint-config-base": "^0.0.5",
    "eslint": "^8.8.0",
    "jest": "^27.4.7",
    "jsdoc-to-markdown": "^7.1.1"
  }
}
